<div class="row-fluid">
  <div class="col-lg-6">
    <div class="outer-div">
    <div class="inner-div">
<h1>New User</h1>
<%= form_for :user, url: users_path do |f| %>
  <%= f.hidden_field :email_address, value: session[:current_user_email_address] %>
  <p>
    <%= f.label :First_Name %>
    <%= f.text_field :first_name %><br>
  </p>

  <p>
    <%= f.label :Last_Name %>
    <%= f.text_field :last_name %><br>
  </p>

  <p>
    <%= f.label :Company %>
    <%= f.text_field :company_name %><br>
  </p>

  <p>
    <%= f.submit "Create Account", class: "btn btn-lg btn-primary"%>
  </p>
</div>
</div>
</div>
<div class="col-lg-6" style="height:100vh;background-image: url(/assets/hel.png);background-color: #21b2fe;background-repeat: no-repeat;">

</div>
</div>
<% end %>
